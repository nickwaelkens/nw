{
  "name": "nw-2016",
  "version": "0.0.1",
  "description": "My personal homepage slash playground. Is it 2016 already?!",
  "main": "src/js/index.js",
  "config": {
    "src": "src",
    "build": "build",
    "dist": "dist"
  },
  "scripts": {
    "server": "live-server $npm_package_config_build --port=4242",
    "release": "npm-run-all release:*",
    "start": "npm run build && npm-run-all --parallel server watch:*",
    "build": "npm-run-all build:*",
    "prebuild": "rimraf $npm_package_config_build && mkdirp $npm_package_config_build",
    "build:coords": "node tools/img2coords.js",
    "build:static": "cpx \"$npm_package_config_src/static/**/*\" $npm_package_config_build",
    "build:js": "webpack --config tools/webpack.config.js",
    "lint:js": "eslint $npm_package_config_src/js",
    "watch": "npm-run-all watch:*",
    "watch:js": "npm run build:js -- --watch",
    "watch:static": "npm run build:static -- -w",
    "test": "npm run lint:js && mocha --recursive --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nickwaelkens/nw.git"
  },
  "author": "Nick Waelkens <hello@nickwaelkens.be>",
  "license": "ISC",
  "homepage": "https://github.com/nickwaelkens/nw",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "bluebird": "^3.3.3",
    "brfs": "^1.4.3",
    "chai": "^3.5.0",
    "cpx": "^1.2.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.0",
    "json-loader": "^0.5.4",
    "live-server": "^0.9.2",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "ncp": "^2.0.0",
    "npm-run-all": "^1.5.1",
    "rimraf": "^2.5.2",
    "transform-loader": "^0.2.3",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "chroma-js": "^1.1.1",
    "lodash": "^4.6.1",
    "pixi.js": "^3.0.9"
  }
}
